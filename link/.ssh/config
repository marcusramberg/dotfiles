Host *

# Forward ssh agent to the remote machine.
ControlMaster auto
ControlPersist yes

Host debook
  ForwardX11Trusted yes
  ForwardX11 yes
  ForwardAgent yes

Host tv
  HostName home.means.no
  User tv
  
Host mtv
  ForwardAgent yes
  HostName home.means.no

Host labs
  ForwardAgent yes
  HostName labs.nordaaker.com

