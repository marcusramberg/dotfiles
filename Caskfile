# This is a Brewfile for phinze/homebrew-cask
# Run `brew bundle Caskfile`

# Install cask
install phinze/cask/brew-cask

# Quick Look plugins
cask install qlcolorcode
cask install qlstephen
cask install qlmarkdown
cask install quicklook-json
cask install quicklook-csv
cask install betterzipql

# Color Picker plugins
cask install colorpicker-hex

# Apps
cask install gitx
cask install iterm2
cask install sequel-pro
cask install osxfuse
cask install google-chrome
cask install kindle
cask install plex-media-server
cask isntall vagrant
cask install inkscape
cask install macvim    
cask install vlc
